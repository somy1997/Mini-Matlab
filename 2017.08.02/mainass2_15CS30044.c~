#include <stdio.h>
#include "myl.h"

int main()
{
	int flag,n;
	float f;
	char str[256];

	//printStr()	
	scanf("%[^\n]s",str);
	flag = printStr(str);
	printf("%d characters printed.\n",flag);
	
	//readFlt and printFlt
	flag=readFlt(&f);
	flag = printFlt(f);
	printf("%d characters printed.\n",flag);
	
	//readInt and printInt
	flag=readInt(&n);
	flag = printInt(n);
	printf("%d characters printed.\n",flag);
	return 0;
}
